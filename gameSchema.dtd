<!ELEMENT dataset (
    Characters*,
    Companies*,
    Companies_Game*,
    Companies_Worker*,
    Cosmetics*,
    Critics*,
    DLC*,
    DLC_Release_Dates*,
    Engines*,
    Game_Genres*,
    Game_Release_Date*,
    Games*,
    Genres*,
    Main_Characters*,
    Platforms*,
    Ratings*,
    Series*,
    Side_Characters*,
    Weapons*,
    Workers*,
